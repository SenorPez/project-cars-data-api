svn up &&
./gradlew build &&
sudo chattr -i ~data-api-app/data-api-0.1-SNAPSHOT.jar &&
sudo rm ~data-api-app/data-api-0.1-SNAPSHOT.jar &&
sudo chown data-api-app:data-api-app build/libs/data-api-0.1-SNAPSHOT.jar &&
sudo chmod 500 build/libs/data-api-0.1-SNAPSHOT.jar &&
sudo mv build/libs/data-api-0.1-SNAPSHOT.jar ~data-api-app/ &&
sudo chattr +i ~data-api-app/data-api-0.1-SNAPSHOT.jar &&
sudo service data-api restart
